{
	"name": "cf-next-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "npm run build-cf && wrangler dev --local --ip 0.0.0.0",
		"build": "npm run build-cf",
		"start": "next start",
		"lint": "next lint",
		"build-cf": "NEXT_PRIVATE_STANDALONE=true NEXT_PRIVATE_OUTPUT_TRACE_ROOT=$PWD next build && opennextjs-cloudflare build --skipNextBuild",
		"predeploy-cf": "drizzle-kit migrate",
		"deploy-cf": "wrangler deploy",
		"prepreview-cf": "drizzle-kit migrate",
		"preview-cf": "wrangler versions upload --env preview",
		"predeploy": "npm run predeploy-cf",
		"deploy": "npm run deploy-cf",
		"prepreview": "npm run prepreview-cf",
		"preview": "npm run preview-cf",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"next:build": "next build",
		"next:dev": "next dev"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.3.0",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.44.6",
		"lucide-react": "^0.474.0",
		"next": "15.4.6",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"tailwind-merge": "^2.5.5"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.19",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"drizzle-kit": "^0.31.5",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"tailwindcss": "^4",
		"tsx": "^4.19.2",
		"typescript": "^5",
		"wrangler": "^4.42.0"
	}
}
